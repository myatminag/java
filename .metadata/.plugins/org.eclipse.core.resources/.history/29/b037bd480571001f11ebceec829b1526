package com.jdc.demo.app;

import java.io.IOException;

import com.jdc.demo.SuperClass;
import com.jdc.demo.app.ApplicationException.Type;

public class DemoApplication {
	
	

	public static void demo(SuperClass obj) {
		try {
			obj.someMethod();
		} catch (IOException e) {
			throw new ApplicationException(Type.Error, "Please check IO related information!");
		}
	}
}
